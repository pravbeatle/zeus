  "description": "VR News for the gods",
  "main": "server.js",
  "engines": {
    "node": "7.8.0",
    "npm": "4.2.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "nodemon server.js --exec babel-node",
